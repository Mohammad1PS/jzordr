        });
        
        // Appointment Form Submission
        const appointmentForm = document.getElementById('appointmentForm');
        const confirmation = document.getElementById('confirmation');
        
        appointmentForm.addEventListener('submit', (e) => {
            e.preventDefault();
            // Here you would typically send the form data to a server
            // For demonstration, we'll just show the confirmation message
            appointmentForm.reset();
            confirmation.classList.remove('hidden');
            setTimeout(() => {
                confirmation.classList.add('hidden');
            }, 5000);
        });
        
        // Contact Form Submission
        const contactForm = document.getElementById('contactForm');
        const contactConfirmation = document.getElementById('contact-confirmation');
        
        contactForm.addEventListener('submit', (e) => {
            e.preventDefault();
            // Here you would typically send the form data to a server
            // For demonstration, we'll just show the confirmation message
            contactForm.reset();
            contactConfirmation.classList.remove('hidden');
            setTimeout(() => {
                contactConfirmation.classList.add('hidden');
            }, 5000);
        });
        
        // Smooth scrolling for anchor links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function(e) {
                e.preventDefault();
                
                mobileMenu.classList.add('hidden');
                
                document.querySelector(this.getAttribute('href')).scrollIntoView({
                    behavior: 'smooth'
                });
            });
        });
